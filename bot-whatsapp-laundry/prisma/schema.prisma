datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Service {
    service_id   Int      @id @default(autoincrement())
    name         String
    hours        Int      @default(0)
    price        Float
    number_phone String?
    status       Int      @default(0)
    createdAt    DateTime @default(now())
    updatedAt    DateTime @updatedAt
}
